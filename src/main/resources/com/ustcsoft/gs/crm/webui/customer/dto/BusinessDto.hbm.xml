<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.ustcsoft.gs.crm.webui.customer.dto.BusinessDto"
        table="Business">
        <id column="businessId" name="businessId" type="int">
            <generator class="native" />
        </id>
        <property name="customerID"></property>
        <property name="businessType"></property>
        <property name="contractYear"></property>
        <property name="contractNumber"></property>
        <property name="contractMoney"></property>
        <property name="isAbolished"></property>
        <property name="startDate"></property>
        <property name="descriptions"></property>
        <property name="createTime"></property>
        <property name="updateTime"></property>
    </class>
</hibernate-mapping>