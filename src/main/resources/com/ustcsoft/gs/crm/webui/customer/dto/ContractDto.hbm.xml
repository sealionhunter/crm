<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.customer.dto.ContractDto"
		table="ContractInfo" >
		<id column="contractID" name="contractID" type="int" >
			<generator class="native" />
		</id>
		<property name="contractName"></property>
		<property name="payEndTime"></property>
		<property name="payType"></property>  
		<property name="orderID"></property>
		<property name="contractValue"></property>
		<property name="fileTemplateName"></property>
		<property name="isAbolished"></property>
	</class>
</hibernate-mapping>