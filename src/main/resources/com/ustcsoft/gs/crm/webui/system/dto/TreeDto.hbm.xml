<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.system.dto.TreeDto" table="Tree">
		<id column="treeID" name="treeID" type="int" >
			<generator class="native" />
		</id>
 		<property name="flag"></property>
		<property name="id"></property>
		<property column="treeName" name="text"></property>
		<property column="isLeaf" name="leaf"></property>
		<property column="isExpanded" name="expanded"></property>
		<property name="fatherID"></property>                  
	</class>
</hibernate-mapping>