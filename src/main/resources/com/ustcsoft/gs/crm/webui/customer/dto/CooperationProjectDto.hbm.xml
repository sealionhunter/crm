<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.customer.dto.CooperationProjectDto"
		table="cooperationProjectInfo" >
		<id column="cooperationProjectID" name="cooperationProjectID" type="int" >
			<generator class="native" />
		</id>
		<property name="cooperatorID"></property>
		<property name="projectName"></property>
		<property name="expectedBeginTime"></property>
		<property name="expectedEndTime"></property>  
		<property name="realBeginTime"></property>
		<property name="realEndTime"></property>
		<property name="projectScale"></property>
		<property name="cooperatorScale"></property>           
		<property name="cooperatorPeopleNumber"></property>     
		<property name="principalWe"></property>
		<property name="principalCooperator"></property>           
		<property name="principalCooperatorPhone"></property>     
		<property name="projectType"></property>
		<property name="projectDetail"></property>           
		<property name="isAbolished"></property> 
		<property name="appraisal"></property>                  
	</class>
</hibernate-mapping>