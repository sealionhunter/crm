<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.ustcsoft.gs.crm.webui.sales.dto.SalesEventDto"
		table="SalesEventInfo">
		<id name='eventID' type="int">
			<generator class="identity" />
		</id>
		<property name="eventName"></property>
		<property name="customerID"></property>
		<property name="contactID"></property>
		<property name="submitterID"></property>
		<property name="status"></property>
		<property name="submitDate"></property>
		<property name="remarks"></property>
		<property name="isAbolished"></property>
	</class>
</hibernate-mapping>