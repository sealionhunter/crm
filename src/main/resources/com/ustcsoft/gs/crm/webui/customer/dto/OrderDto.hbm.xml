<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.customer.dto.OrderDto"
		table="orderInfo" >
		
		<id column="id" name="id" type="int" ><generator class="native" /></id>
		<property name="orderID"></property>
		<property name="customerID"></property>
		<property name="orderState"></property>
		<property name="contactID"></property>
		<property name="ourRepresentative"></property>
		<property name="deliveryDate"></property>
		<property name="transactionPrice"></property>
		<property name="remark"></property>
		<property name="isAbolished"></property>
		<property name="ourTelephone"></property>
		<property name="type"></property>
		<property name="customerContact"></property>
		<property name="eventID"></property>
	</class>
</hibernate-mapping>