<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
 <class name="com.ustcsoft.gs.crm.webui.customer.dto.SourceInfoDto"
        table="CustomerSourceInfo" >
        <id name="sourceID" type="int" column="sourceID">
             <generator class="native" />
        </id>
        <property name="customerID"></property>
        <property name="sourceArea"></property>
        <property name="sourceType"></property>
        <property name="isAbolished"></property>
        <property name="descriptions"></property>                 
    </class>
</hibernate-mapping>