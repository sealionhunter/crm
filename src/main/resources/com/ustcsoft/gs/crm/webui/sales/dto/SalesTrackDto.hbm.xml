<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ustcsoft.gs.crm.webui.sales.dto.SalesTrackDto" table="SalesTrackInfo" dynamic-update="true">
        <id name="salesTrackNo" type="java.lang.Integer">
            <column name="salesTrackNo" />
            <generator class="native"></generator>
        </id>
        <property name="submitterID" type="java.lang.Integer">
            <column name="submitterID" not-null="true" />
        </property>
        <property name="eventID" type="java.lang.Integer">
            <column name="eventID" not-null="true" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="status" not-null="true" />
        </property>
        <property name="submitDate">
        	<column name="submitDate" not-null="true" />
        </property>
        <property name="customerID" type="java.lang.Integer">
            <column name="customerID" not-null="true"/>
        </property>
        <property name="isAbolished">
            <column name="isAbolished" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
