<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
    <class name="com.ustcsoft.gs.crm.webui.customer.dto.ContactTrackInfoDto"
        table="ContactTrackInfo" >
        <id column="contactID" name="contactID" type="int" >
            <generator class="native" />
        </id>
        <property name="customerID"></property>
        <property name="contactTheme"></property>
        <property name="weContact"></property>
        <property name="oppositeContact"></property>
        <property name="contactWay"></property>
        <property name="contactContent"></property>
        <property name="contactType"></property>
        <property name="planDateBegin"></property>
        <property name="realityDateBegin"></property>
        <property name="realityDateEnd"></property>
        <property name="userFeedbackInformation"></property>
        <property name="strategy"></property>    
        <property name="notContantReason"></property>
        <property name="ifContact"></property>
        <property name="isAbolished"></property> 
        <property name="remarks"></property> 
<!--         <property name="eventID"></property>  -->
<!--         <property name="state"></property>  -->
<!--         <property name="checkResult"></property>  -->
        <property name="createTime"></property>
        <property name="updateTime"></property>
    </class>
</hibernate-mapping>