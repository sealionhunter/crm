<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.customer.dto.CompetitorinfoDto"
		table="CompetitorInfo" >
		<id column="competitorInfoId" name="competitorInfoId" type="int" >
			<generator class="native" />
		</id>
		<property name="competitorName"></property>
		<property name="competitorField"></property>
		<property name="competitorProperty"></property>
		<property name="competitorLocation"></property>
		<property name="competitorFoundDate"></property>
		<property name="competitorMoney"></property>
		<property name="competitorPeople"></property>          
		<property name="competitorDescription"></property>
		<property name="competitorType"></property>
		<property name="isAbolished"></property>    
		<property name="competitorDetail"></property>    			
	</class>
</hibernate-mapping>