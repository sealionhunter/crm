<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.customer.dto.SelectProductDto"
		table="SelectProductInfo" >
		<id column="selectProductID" name="selectProductID" type="int" > <generator class="native" /></id>
		<property name="id"></property>
		<property name="orderID"></property>
		<property name="productID"></property>
		<property name="name"></property>
		<property name="price"></property>
		<property name="productNumber"></property>
		<property name="discount"></property>
		<property name="isAbolished"></property>
	</class>
</hibernate-mapping>