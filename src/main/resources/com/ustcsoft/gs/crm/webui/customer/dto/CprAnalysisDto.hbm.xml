<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >
	<class name="com.ustcsoft.gs.crm.webui.customer.dto.CprAnalysisDto"
		table="CprAnalysis" >
		<id column="cprAnalysisID" name="cprAnalysisID" type="int" >
			<generator class="native" />
		</id>
		<property name="competitorInfoId"></property>
		<property name="area"></property>
		<property name="ability"></property>
		<property name="targets"></property>
		<property name="strategy"></property>
		<property name="prediction"></property> 
		<property name="advantage"></property>          
		<property name="disadvantage"></property>    
		<property name="advAnalysis"></property>    
		<property name="disadvAnalysis"></property>    	
		<property name="others"></property>    
		<property name="compositeComp"></property>    
		<property name="compositeDefense"></property>
		<property name="cprAnalysisTime"></property>  
		<property name="advice"></property> 
		<property name="isAbolished"></property>               
	</class>
</hibernate-mapping>